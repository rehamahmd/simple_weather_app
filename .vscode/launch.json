    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "flutter_starter_kit",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main/dev.dart",
            "args": ["--flavor", "dev"]
        },
        {
            "name": "flutter_starter_kit (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main/staging.dart",
            "args": ["--flavor", "staging"]
        },
        {
            "name": "flutter_starter_kit (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main/production.dart",
            "args": ["--flavor", "production"]
        }
    ]
}